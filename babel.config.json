{
  "presets": [
    "@vue/babel-preset-jsx",
    [
      "@babel/env",
      {
        "targets": {
          "edge": "17",
          "firefox": "60",
          "chrome": "67",
          "safari": "11.1"
        }
      }
    ]
  ],
  "env": {
    "utils": {
      "presets": [
        "@vue/babel-preset-jsx",
        [
          "@babel/env",
          {
            "targets": {
              "edge": "17",
              "firefox": "60",
              "chrome": "67",
              "safari": "11.1"
            }
          }
        ]
      ],
      "plugins": [
        ["module-resolver", {
          "root": ["element-nice-ui"],
          "alias": {
            "element-ui/src": "element-nice-ui/lib"
          }
        }]
      ]
    }
  }
}
